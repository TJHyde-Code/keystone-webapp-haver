@model IEnumerable<HaverGroupProject.Models.Note>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PreOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scope)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BudgetAssembHrs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActualAssembHours)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActualReworkHours)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OtherComments)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OperationsSchedule)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PreOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Scope)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BudgetAssembHrs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActualAssembHours)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActualReworkHours)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OtherComments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OperationsSchedule.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
