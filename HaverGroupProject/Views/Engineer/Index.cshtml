@model IEnumerable<HaverGroupProject.Models.Engineer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EngFirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngMiddleName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngLastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EngEmail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EngFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngMiddleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
